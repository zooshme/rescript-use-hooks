rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o Hooks-UseHooks.cmi : cp ../bs/./src/Hooks-UseHooks.cmi
o Hooks-UseHooks.cmj : cp ../bs/./src/Hooks-UseHooks.cmj
o Hooks-UseHooks.cmt : cp ../bs/./src/Hooks-UseHooks.cmt
o Hooks.res : cp ../.././src/Hooks.res
o UseHooks.cmi : cp ../bs/UseHooks.cmi
o UseHooks.cmj : cp ../bs/UseHooks.cmj
o UseHooks.cmt : cp ../bs/UseHooks.cmt
build install.stamp : touch Hooks-UseHooks.cmi Hooks-UseHooks.cmj UseHooks.cmi UseHooks.cmj
